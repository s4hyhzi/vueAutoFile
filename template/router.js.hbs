import Vue from 'vue'
import Router from 'vue-router'
{{#each list}}
import {{name}} from "../{{path}}"
{{/each}}
Vue.use(Router)

export default new Router({
  mode: 'history',
  base: process.env.BASE_URL,
  routes: [
    {{#each list}}
    {
      path: '{{url}}',
      name: '{{name}}',
      component: {{name}}
    },
    {{/each}}
  ]
})
